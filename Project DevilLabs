local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Darkest Legends ScriptHub!", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--Notifications

OrionLib:MakeNotification({
	Name = "Welcome!",
	Content = "This scripthub was encorged by some friends of mine.",
	Image = "rbxassetid://4483345998",
	Time = 10
})

OrionLib:MakeNotification({
